    <validationRules>
        <fullName>OMSProductName_no_Special_Characters</fullName>
        <active>false</active>
        <description>Validation Rule to ensure no special char are entered.</description>
        <errorConditionFormula>NOT( REGEX( OMSProductName__c, &quot;[a-zA-Z0-9]+&quot;))</errorConditionFormula>
        <errorDisplayField>OMSProductName__c</errorDisplayField>
        <errorMessage>Special Characters not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tariff_Code_Format</fullName>
        <active>false</active>
        <errorConditionFormula>!REGEX(Tariff_Code__c,&quot;^(\\d{4}\\.\\d{2}.\\d{4})?$&quot;)</errorConditionFormula>
        <errorDisplayField>ccrz__Product__c</errorDisplayField>
        <errorMessage>Please enter Tariff Code in the following format: 0000.00.0000</errorMessage>
    </validationRules>
