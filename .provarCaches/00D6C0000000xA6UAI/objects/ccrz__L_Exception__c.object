    <validationRules>
        <fullName>ccrz__Exception_Message_Required</fullName>
        <active>true</active>
        <description>The exception message with details is required</description>
        <errorConditionFormula>OR(
       ISBLANK( ccrz__Message__c ) 
    , ISNULL(ccrz__Message__c )
)</errorConditionFormula>
        <errorDisplayField>ccrz__Message__c</errorDisplayField>
        <errorMessage>Exception Message is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ccrz__PageSeverityReq_UserMessage</fullName>
        <active>true</active>
        <description>Page severity needs to be defined for every user message</description>
        <errorConditionFormula>AND (
    ccrz__Show_EndUser__c 
 ,  OR
          (  ISBLANK(ccrz__Page_Severity__c)
            , ISNULL(ccrz__Page_Severity__c)
          )
)</errorConditionFormula>
        <errorDisplayField>ccrz__Page_Severity__c</errorDisplayField>
        <errorMessage>Page Severity is required when show end user flag is set</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ccrz__UserFriendlyMessageReq_UI_Errors</fullName>
        <active>true</active>
        <description>A user friendly message is required, when the message needs to be displayed in the UI</description>
        <errorConditionFormula>AND ( ccrz__Show_EndUser__c , 
            OR (
                      ISBLANK( ccrz__User_Friendly_Message__c )
                    , ISNULL( ccrz__User_Friendly_Message__c )
                 )
   )</errorConditionFormula>
        <errorDisplayField>ccrz__User_Friendly_Message__c</errorDisplayField>
        <errorMessage>User friendly message is required, if show end user flag is set to true</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
