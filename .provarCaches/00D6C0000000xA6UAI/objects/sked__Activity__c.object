    <validationRules>
        <fullName>sked__DateRangeValidation</fullName>
        <active>false</active>
        <description>Minutes from start to end must be longer than 0 but no greater than 24 hrs</description>
        <errorConditionFormula>OR(
   ROUND(((sked__End__c - sked__Start__c) * 1440),0) &gt; 1440,
   ROUND(((sked__End__c - sked__Start__c) * 1440),0) &lt;= 0.0
)</errorConditionFormula>
        <errorMessage>Activity date range must be greater than zero but no greater than 24 hrs</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__QuantityNullOrPositive</fullName>
        <active>true</active>
        <description>Quantity of Resources required must be null or greater than 0</description>
        <errorConditionFormula>AND( NOT( ISNULL( sked__Quantity__c ) ) , sked__Quantity__c  &lt;= 0 )</errorConditionFormula>
        <errorDisplayField>sked__Quantity__c</errorDisplayField>
        <errorMessage>Quantity of Resources required must be null or greater than 0</errorMessage>
    </validationRules>
