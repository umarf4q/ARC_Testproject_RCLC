    <validationRules>
        <fullName>sked__EndTimeNotAfterJobFinish</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISNULL( sked__Scheduled_End1__c ) ) , NOT( ISNULL( sked__Job__r.sked__Finish__c ) ), sked__Scheduled_End1__c &gt; sked__Job__r.sked__Finish__c )</errorConditionFormula>
        <errorMessage>End time cannot be after job end time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__QuantityGreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>sked__Quantity__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>sked__Quantity__c</errorDisplayField>
        <errorMessage>Quantity must be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__RelativeStartNullIfTimeSourceFalse</fullName>
        <active>true</active>
        <errorConditionFormula>AND( sked__Job_Allocation_Time_Source__c = false, NOT( ISNULL( sked__Relative_Start__c ) ) )</errorConditionFormula>
        <errorDisplayField>sked__Relative_Start__c</errorDisplayField>
        <errorMessage>Relative Start must be null if Job Allocation Time Source is false</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__StartTimeNotBeforeJobStart</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISNULL( sked__Scheduled_Start1__c ) ) , NOT( ISNULL( sked__Job__r.sked__Start__c ) ), sked__Scheduled_Start1__c &lt; sked__Job__r.sked__Start__c )</errorConditionFormula>
        <errorMessage>Start time cannot be before job start time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__TimeSourceMustHaveDuration</fullName>
        <active>true</active>
        <errorConditionFormula>AND( sked__Job_Allocation_Time_Source__c  = true , ISNULL( sked__Duration__c ) )</errorConditionFormula>
        <errorDisplayField>sked__Duration__c</errorDisplayField>
        <errorMessage>Duration must be set if Job Allocation Time Source is true</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
