    <validationRules>
        <fullName>sked__DurationMustBeGreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>sked__Duration__c  &lt; 1</errorConditionFormula>
        <errorDisplayField>sked__Duration__c</errorDisplayField>
        <errorMessage>Shift duration must be greater than 0 minute</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__DurationMustMatchStartToEnd</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISNULL( sked__End1__c ) ) , NOT( ISNULL( sked__Start__c ) ), sked__Duration__c  &lt;&gt;  ROUND( (sked__End1__c  -  sked__Start__c) * 1440 ,0)  )</errorConditionFormula>
        <errorMessage>Duration must match Start to End</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sked__StartEndDurationAreRequired</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL( sked__Start__c ) , ISNULL( sked__End1__c ) , ISNULL( sked__Duration__c ) )</errorConditionFormula>
        <errorMessage>Start, End and Duration are required</errorMessage>
    </validationRules>
