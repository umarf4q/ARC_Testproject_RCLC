<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This Custom Setting holds field mapping between Salesforce Commerce Cloud Order Object and Service Cloud Order Object. This mapping is used in Rest Apex wrapper class to create order header in Salesforce.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>NSSCCConnector__Enable_Sync__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This field enable if this attribute will be synchronised from Commerce Cloud through call back.</description>
        <externalId>false</externalId>
        <inlineHelpText>This field enable if this attribute will be synchronised from Commerce Cloud through call back.</inlineHelpText>
        <label>Enable Sync</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>NSSCCConnector__Field_Api_Name__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <description>This Field holds mapping between Service Cloud Object Attributes with Commerce Cloud request object attributes.</description>
        <externalId>false</externalId>
        <label>Field Api Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>OrderFieldMapping</label>
    <visibility>Public</visibility>
</CustomObject>
